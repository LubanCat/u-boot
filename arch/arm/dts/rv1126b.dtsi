		nvmem-cells = <&npu_leakage>;
		nvmem-cell-names = "leakage";

		opp-396000000 {
			opp-hz = /bits/ 64 <396000000>;
			opp-microvolt = <900000 900000 1000000>;
		};
		opp-594000000 {
			opp-hz = /bits/ 64 <594000000>;
			opp-microvolt = <900000 900000 1000000>;
		};
	};

